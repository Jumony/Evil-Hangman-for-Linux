nt**)malloc(sizeof(int*) * rows);

  for (int i = 0; i < columns; i++)
    {
      grid[i] = (int*)malloc(sizeof(int) * columns);
    }

  for (int i = 0; i < rows; i++)
    {
      for (int j = 0; j < rows; j++)
	{
	  grid[i][j] = i + j;
	}
    }
  
  for (int i = 0; i < rows; i++)
    {
      free(grid[i]);
    }

  for (int i = 0; i < rows; i++)
    {
      for (int j = 0; j < columns; j++)
	{
	  printf("%3d", grid[i][j]);
	}
      printf("\n");
    }

  for (int i = 0; i < rows; i++)
    {
      free(grid[i]);
    }
  free(grid);
}
